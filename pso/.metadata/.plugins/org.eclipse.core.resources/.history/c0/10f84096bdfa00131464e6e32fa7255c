package swarm;

import java.util.Vector;

public class cluster {
	Vector<Float> centroid ;
	Vector< Vector<Float> > near_points ; 
	public cluster(Vector<Float> centroid_val)
	{
		centroid = new Vector<>(centroid_val);
		near_points = new Vector<Vector<Float> >() ; 
	}
	
	public void add_point(Vector <Float> point) 
	{
		near_points.add(point);
	}
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
